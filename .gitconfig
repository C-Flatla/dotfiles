[user]
	name = Cody Flatla
[core]
	excludesfile = ~/.gitignore_global
[alias]
	co = checkout
	b = branch
	s = status
	l = log
[push]
	default = current
[pull]
	rebase = false

# Don't add any configuration below this line
# This is to support a local gitconfig file
[include]
	path = .gitconfig.local
